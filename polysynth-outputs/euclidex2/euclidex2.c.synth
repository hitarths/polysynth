real a,b,p,q,r,s,x,y;
@pre((x >= 0 and y >= 0));
a = x;
b = y;
p = 1;
q = 0;
r = 0;
s = 1;
while (1 >= 0, (a == ((y * r) + (x * p)) and b == ((x * q) + (y * s)))) {
if (a >= b) {
a = (0)*(1)+(-1)*(b)+(1)*(a);
p = (0)*(1)+(1)*(p)+(-1)*(q);
r = (r - s);
}
else {
b = (0)*(1)+(1)*(b)+(-1)*(a);
q = (q - p);
s = (0)*(1)+(-1)*(r)+(1)*(s);
}
}
@post(1 >= 0);
