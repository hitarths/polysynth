real a,b,x,y,u,v;
@pre((a >= 0 and b >= 0));
x = a;
y = b;
u = b;
v = 0;
while (1 >= 0, ((x * u) + (y * v)) == (a * b)) {
while (1 >= 0, ((x * u) + (y * v)) == (a * b)) {
x = (1)*(x)+(0)*(1)+(-1)*(y);
v = (v + u);
}
while (1 >= 0, ((x * u) + (y * v)) == (a * b)) {
y = (y - x);
u = (0)*(1)+(1)*(v)+(1)*(u);
}
}
@post(1 >= 0);
